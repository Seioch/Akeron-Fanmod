<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--=============================== Paper Decor ===================================-->

<ThingDef ParentName="BuildingBase">
	<defName>AKN_WallPapersA</defName>
	<label>Wall Papers A</label>
	<thingClass>Building</thingClass>
	
	<clearBuildingArea>false</clearBuildingArea>

	<description>"A few nice drawings someone misplaced? They look good enough to hang up!"</description>	
	<uiIconPath>UI/AKN_WallPapersAUI</uiIconPath>
	<graphicData>
	  <texPath>Things/Building/Papers/AKN_WallPapersA</texPath>
	  <graphicClass>Graphic_Multi</graphicClass>
	  <shaderType>Transparent</shaderType>
      <drawSize>(1,1)</drawSize>
	</graphicData>
	<costList>
      <WoodLog>5</WoodLog>
    </costList>
	<rotatable>true</rotatable>
	<defaultPlacingRot>South</defaultPlacingRot>
	<building>
	  <isEdifice>false</isEdifice>
	  <canPlaceOverWall>true</canPlaceOverWall>
	</building>
	<blockWind>false</blockWind>
	<altitudeLayer>BuildingOnTop</altitudeLayer>	
	<passability>Standable</passability>
	<pathCost>3</pathCost>
	<fillPercent>0</fillPercent>
	<statBases>
	  <MaxHitPoints>5</MaxHitPoints>
	  <WorkToBuild>600</WorkToBuild>
	  <Flammability>1</Flammability>
	  <Beauty>0</Beauty>
	  <Mass>0.01</Mass>	  
	</statBases>
	<designationCategory>Misc</designationCategory>
	<designatorDropdown>AKN_PapersDropdown</designatorDropdown>
	<researchPrerequisites>
	  <li>AKN_AdorableDoodles</li>
	</researchPrerequisites>
	<comps>
	  <li Class="CompProperties_Facility">
	    <statOffsets>
		  <ResearchSpeedFactor>0.01</ResearchSpeedFactor>
		</statOffsets>
		<maxSimultaneous>1</maxSimultaneous>
		<maxDistance>10</maxDistance>
	  </li>
	</comps>
	<placeWorkers>
	  <li>PlaceWorker_ShowFacilitiesConnections</li>
	</placeWorkers>
		
</ThingDef>

